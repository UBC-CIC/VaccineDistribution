    "react-redux": "^7.2.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "^3.4.4",
    "react-search": "^2.1.1",
    "react-select": "^3.2.0",
    "reactstrap": "8.4.1",
    "redux": "^4.0.5",
    "uuid": "^8.3.2"
