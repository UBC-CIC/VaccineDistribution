  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@aws-amplify/ui-react": "^0.2.34",
    "@fortawesome/fontawesome-free": "5.12.1",
